    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "cwd": "${workspaceFolder}/backend",
            "args": [
                "open_webui.main:app",
                "--port=8080",
                "--host=0.0.0.0",
                "--forwarded-allow-ips='*'",
                "--reload",
            ],
            "jinja": true
        }
    ]
}