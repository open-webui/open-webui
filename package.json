{
	"name": "open-webui",
	"version": "0.5.7-ccv2-1.11.2",
	"private": true,
	"scripts": {
		"start": "npm run backend:check && bash -c 'env $(grep -v '^#' '${ENV_FILE:-.env}'| xargs) backend/start.sh'",
		"start:build": "npm run build && npm run start",
		"backend:setup": "npm run backend:check && pip install -r backend/requirements.txt",
		"backend:check": "python --version | grep -q 'Python 3.11' || (echo 'Error: Python 3.11 required' && exit 1)",
		"test": "bash -c 'env $(grep -v '^#' '${ENV_FILE:-.env}'| xargs) npx playwright test'",
		"test:en": "bash -c 'env $(grep -v '^#' '${ENV_FILE:-.env}'| xargs) npx playwright test --project=chromium-en --project=firefox-en'",
		"test:fr": "bash -c 'env $(grep -v '^#' '${ENV_FILE:-.env}'| xargs) npx playwright test --project=chromium-fr --project=firefox-fr'",
		"test:debug": "bash -c 'env $(grep -v '^#' '${ENV_FILE:-.env}'| xargs) npx playwright test --project=chromium-en'",
		"test:headed": "bash -c 'env $(grep -v '^#' '${ENV_FILE:-.env}'| xargs) npx playwright test --headed'",
		"test:report": "npx playwright show-report",
		"test:setup": "npx playwright install --with-deps --force",
		"test:setup:headless": "npx playwright install --with-deps --only-shell --force",
		"coverage:build": "export NODE_OPTIONS='--max-old-space-size=8192' && VITE_COVERAGE=true npm run build",
		"coverage:test": "npm run coverage:clean && npm run test && npx nyc report",
		"coverage:clean": "rm -rf .nyc_output coverage",
		"setup": "npm run test:setup && npm run backend:setup && npm run build",
		"dev": "npm run pyodide:fetch && vite dev --host",
		"dev:5050": "npm run pyodide:fetch && vite dev --port 5050",
		"build": "npm run pyodide:fetch && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "npm run lint:frontend ; npm run lint:types ; npm run lint:backend",
		"lint:frontend": "eslint . --fix",
		"lint:types": "npm run check",
		"lint:backend": "pylint backend/",
		"format": "prettier --plugin-search-dir --write \"**/*.{js,ts,svelte,css,md,html,json}\"",
		"format:backend": "black . --exclude \".venv/|/venv/\"",
		"i18n:parse": "i18next --config i18next-parser.config.ts && prettier --write \"src/lib/i18n/**/*.{js,json}\"",
		"test:frontend": "vitest --passWithNoTests",
		"pyodide:fetch": "node scripts/prepare-pyodide.js"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.2",
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-auto": "3.2.2",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tailwindcss/typography": "^0.5.13",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"i18next-parser": "^9.0.1",
		"nyc": "^17.1.0",
		"postcss": "^8.4.31",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"require": "^2.4.20",
		"sass-embedded": "^1.81.0",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"svelte-confetti": "^1.3.2",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.5.4",
		"vite": "^5.4.21",
		"vite-plugin-istanbul": "^7.2.1",
		"vitest": "^4.0.15"
	},
	"type": "module",
