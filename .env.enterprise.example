# Open WebUI Enterprise defaults
# Copy to ".env" or pass with docker compose --env-file .env.enterprise

# App mode
ENTERPRISE_MODE=true
LOCAL_MODE_DISABLED=true
BYPASS_EMBEDDING_AND_RETRIEVAL=false

# Engines (SaaS only)
RAG_EMBEDDING_ENGINE=openai
RAG_RERANKING_ENGINE=
RAG_RERANKING_MODEL=reranker
AUDIO_STT_ENGINE=openai
AUDIO_TTS_ENGINE=openai
VECTOR_DB=chroma
OCR_ENGINE=azure_document_intelligence

# Optional: Build-time hints (used for direct docker build; compose sets these for you)
USE_ENTERPRISE_MODE=true
USE_SLIM=true
USE_CUDA=false

# Network / Ports
OPEN_WEBUI_PORT=3000

# App DB defaults to SQLite; no Postgres needed by default in enterprise.

# Provider credentials (left blank on purpose; app starts without them)
OPENAI_API_KEY=
RAG_OPENAI_API_KEY=
RAG_AZURE_OPENAI_API_KEY=

AUDIO_STT_OPENAI_API_KEY=
AUDIO_STT_AZURE_API_KEY=
AUDIO_STT_AZURE_REGION=

AUDIO_TTS_OPENAI_API_KEY=
AUDIO_TTS_API_KEY=

# External reranker (HTTP)
RAG_EXTERNAL_RERANKER_URL=
RAG_EXTERNAL_RERANKER_API_KEY=

# OCR providers (choose one; both may remain blank for startup)
DOCUMENT_INTELLIGENCE_ENDPOINT=
DOCUMENT_INTELLIGENCE_KEY=
AWS_TEXTRACT_ACCESS_KEY_ID=
AWS_TEXTRACT_SECRET_ACCESS_KEY=
AWS_TEXTRACT_SESSION_TOKEN=
AWS_TEXTRACT_REGION=

# Telemetry
SCARF_NO_ANALYTICS=true
DO_NOT_TRACK=true
ANONYMIZED_TELEMETRY=false
